<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Nullable>enable</Nullable>
    <PackageVersion>0.0.6</PackageVersion>
    <Title>Luger.LoggerProvider</Title>
    <Authors>Meyhem</Authors>
    <Description>Microsoft.Extensions.Logging.ILoggerProvider implementation for logging into Luger server</Description>
    <Copyright>Meyhem</Copyright>
    <PackageProjectUrl>https://github.com/Meyhem/Luger.LoggerProvider</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/Meyhem/Luger.LoggerProvider</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>luger log provider</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="5.0.0" />
  </ItemGroup>

</Project>
